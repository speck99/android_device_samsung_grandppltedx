# system.prop for grandpplte

# Nougat stuff
config.disable_consumerir=true
media.sf.omx-plugin=libffmpeg_omx.so,libsomxcore.so

# Platform
ro.mediatek.chip_ver=S01
ro.mediatek.platform=MT6737T
ro.mediatek.version.branch=alps-mp-m1.mp8.tc10sp
ro.mediatek.version.release=alps-mp-m1.mp8.tc10sp
ro.mediatek.version.sdk=4

# RIL
rild.libpath=/system/lib/libsec-ril.so
rild.libpath2=/system/lib/libsec-ril-dsds.so
rild.libargs=-d /dev/ttyC0

ro.telephony.ril_class=grandpplteRIL
#ro.telephony.ril.config=fakeiccid
ro.telephony.sim.count=2
ro.telephony.default_network=9
ro.boot.opt_lte_support=1
telephony.lteOnGsmDevice=1
telephony.lteOnCdmaDevice=0

ro.mtk_lte_support=1
ro.mtk_sim_hot_swap=1
ro.mtk_gemini_support=1
ro.mediatek.gemini_support=true
ro.gemini.smart_sim_switch=false
ro.ril.hsxpa=1
ro.ril.gprsclass=10

ril.current.share_modem=2
ril.first.md=1
ril.flightmode.poweroffMD=0
ril.telephony.mode=0
ril.external.md=0
ril.specific.sm_cause=0

persist.radio.multisim.config=dsds
persist.radio.gemini_support=1
persist.radio.fd.counter=15
persist.radio.fd.off.counter=5
persist.radio.fd.r8.counter=15
persist.radio.fd.off.r8.counter=5
persist.radio.sib16_support=0

fmradio.driver.enable=1
ro.mtk_fm_recording_support=1

# audio
ro.camera.sound.forced=0
ro.audio.silent=0

# VoLTE
ro.mtk.volte.enable=1
ro.mtk_volte_support=1

persist.dbg.volte_avail_ovr=1

# IMS
ro.mtk_ims_support=1

# SELinux
# DISABLE FOR TESTING PURPOSE ONLY
ro.build.selinux=1

# Dalvik / hwui
ro.hwui.texture_cache_size=24
ro.hwui.layer_cache_size=16
ro.hwui.path_cache_size=4
ro.hwui.texture_cache_flushrate=0.4
ro.hwui.shape_cache_size=1
ro.hwui.gradient_cache_size=0.5
ro.hwui.drop_shadow_cache_size=2
ro.hwui.r_buffer_cache_size=2
ro.hwui.text_small_cache_width=512
ro.hwui.text_small_cache_height=512
ro.hwui.text_large_cache_width=1024
ro.hwui.text_large_cache_height=1024

#dalvik.vm.heapgrowthlimit=128m
#dalvik.vm.heapsize=512m

dalvik.vm.mtk-stack-trace-file=/data/anr/mtk_traces.txt

# from wileyfox_porridge
dalvik.vm.heapstartsize=8m
dalvik.vm.heapgrowthlimit=128m
dalvik.vm.heapsize=512m
dalvik.vm.heaptargetutilization=0.75
dalvik.vm.heapminfree=512k
dalvik.vm.heapmaxfree=8m

# Block
#ro.frp.pst=/dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/PERSISTENT

# PQ
ro.mtk_pq_support=2

# Wi-Fi / BT
wifi.interface=wlan0
wifi.direct.interface=p2p0
wifi.tethering.interface=ap0

ro.mtk_wlan_support=1
ro.mtk_wifi_mcc_support=1
ro.mediatek.wlan.wsc=1
ro.mtk_dhcpv6c_wifi=1

mediatek.wlan.chip=CONSYS_MT6735
mediatek.wlan.module.postfix=_consys_mt6735
mediatek.wlan.ctia=0

#-
ro.mtk_bt_support=1

# Tethering
ro.mtk_tetheringipv6_support=1

# GPS
ro.mtk_agps_app=1
ro.mtk_gps_support=1

#+ Other - mix of Samsung and MTK-common
ro.sf.lcd_density=240
persist.sys.lcd_density=240

ro.opengles.version=196609

debug.hwui.render_dirty_regions=false

ro.mtk_world_phone_policy=0
ro.kernel.zio=38,108,105,16
ro.sf.hwrotation=0
ro.mtk_emmc_support=1
persist.mtk.wcn.combo.chipid=-1
ro.com.android.mobiledata=true
ro.mtk_enable_md1=1
ro.mtk_flight_mode_power_off_md=1
ro.mtk_world_phone=1
ro.mtk_cta_set=1
ro.mtk_rild_read_imsi=1
ro.sys.usb.mtp.whql.enable=0
sys.ipo.pwrdncap=2
ro.sys.usb.bicr=no
ro.sys.usb.charging.only=yes
ro.sys.usb.storage.type=mtp,mass_storage

ro.camera.sound.forced=0
ro.audio.silent=0

ro.zygote.preload.enable=0
persist.af.audio_patch_fm=1
persist.md.perm.checked=1

# Test
qemu.hw.mainkeys=0
